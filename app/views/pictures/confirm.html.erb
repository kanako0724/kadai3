<h3>以下の内容で送信します</h3>
<%= form_with(model: @picture,url: pictures_path,local: true) do |form| %>
<div class="content-wrapper">
  <% if @picture.comment.present? %>
    <%= form.hidden_field :comment %>
    <p><strong>comment:</strong><%= @picture.comment %></p>
  <% end %>
    <%= image_tag(@picture.picture.url) %>
    <%= hidden_field_tag :"cache[picture]",@picture.picture.cache_name %>
</div>
    <%= form.submit "登録する"%>
<% end %>

<%= form_with(model: @picture,url: new_picture_path,local: true,method: 'get') do |form|%>
  <%= form.hidden_field :picture %>
  <%= hidden_field :"cache[picture]",@picture.picture.cache_name %>
  <%= form.submit '戻る',name: 'back' %>
<% end %>